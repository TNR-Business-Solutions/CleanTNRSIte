{
  "version": 2,
  "builds": [
    {
      "src": "privacy-policy.html",
      "use": "@vercel/static"
    },
    {
      "src": "terms-conditions.html",
      "use": "@vercel/static"
    },
    {
      "src": "assets/styles.css",
      "use": "@vercel/static"
    },
    {
      "src": "media/logo.png",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/privacy-policy.html",
      "dest": "/privacy-policy.html"
    },
    {
      "src": "/terms-conditions.html",
      "dest": "/terms-conditions.html"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/",
      "dest": "/privacy-policy.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
