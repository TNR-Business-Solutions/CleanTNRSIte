{
  "version": 2,
  "buildCommand": "node verify-images.js",
  "outputDirectory": ".",
  "functions": {
    "api/**/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/submit-form",
      "destination": "/api/submit-form"
    },
    {
      "source": "/checkout",
      "destination": "/api/checkout"
    }
  ]
}